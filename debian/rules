#!/usr/bin/make -f
%:
	dh $@

# skip "make clean" to avoid dependency on cito
override_dh_auto_clean:

override_dh_auto_install:
	$(MAKE) PREFIX=$$(pwd)/debian/tmp/usr libdir=$$(pwd)/debian/tmp/usr/lib XNVIEW=$$(pwd)/debian/tmp/opt/XnView BUILDING_PACKAGE=1 install-gimp install-thumbnailer install-xnview
