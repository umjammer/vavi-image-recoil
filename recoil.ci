		case PACKED_EXT("XIMG"):
			return DecodeStImg(content, contentLength);
		case PACKED_EXT("IMN"):
			return DecodeAtari8Ice(content, contentLength, false, 18);
		case PACKED_EXT("ING"):
		case PACKED_EXT("INP"):
			return DecodeInp(content, contentLength);
		case PACKED_EXT("INT"):
			return DecodeInt(content, contentLength)
