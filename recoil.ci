		case PACKED_EXT("CCI"):
			return DecodeCci(content, contentLength);
		case PACKED_EXT("CDU"):
			return contentLength == 10277 && DecodeC64Multicolor(content, 0x113, 0x2053, 0x243b, content[0x2823]);
		case PACKED_EXT("CH4"):
		case PACKED_EXT("CH6"):
		case PACKED_EXT("CH8"):
			return DecodeCh8(content, contentLength);
		case PACKED_EXT("CHE"):
