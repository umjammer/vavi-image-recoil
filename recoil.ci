			return DecodeAtari8Ice(content, contentLength, false, 2);
		case PACKED_EXT("IRG"):
			return DecodeAtari8Ice(content, contentLength, false, 1);
		case PACKED_EXT("ISM"):
			return contentLength == 10218 && DecodeC64Multicolor(content, 0x402, 0x2402, 2, content[0x2401]);
		case PACKED_EXT("IST"):
			return DecodeIst(content, contentLength);
		case PACKED_EXT("JGP"):
			return DecodeJgp(content, contentLength);
		case PACKED_EXT("KOA"):
		case PACKED_EXT("GIG"):
		case PACKED_EXT("RPM"):
			return DecodeKoa(content, contentLength);
		case PACKED_EXT("LEO"):
			return DecodeLeo(content, contentLength);
		case PACKED_EXT("MAX"):
			return DecodeMax(content, contentLength);
		case PACKED_EXT("MBG"):
			return DecodeMbg(content, contentLength);
		case PACKED_EXT("MC"):
			return DecodeMc(content, contentLength);
		case PACKED_EXT("MCH"):
			return DecodeMch(content, contentLength);
		case PACKED_EXT("MCP"):
			return DecodeMcp(content, contentLength);
		case PACKED_EXT("MCS"):
			return DecodeMcs(content, contentLength);
		case PACKED_EXT("MG1"):
		case PACKED_EXT("MG2"):
